     min-width: 480px;
  }
  .container-fluid > .sidebar {
    float: left;
    width: 164px;
    left: 20px;
    position: absolute;
    display: block;
  }
  .container-fluid > .content {
    margin-left: 180px;
  }
  ul.nav li#home_link {
    display:none;
  }
  article {
    padding: 8px 20px;
  }
  article h1 {
    font-size: 26px;
    line-height: 32px;
    margin-bottom: 12px;
  }
