  .container-fluid > .content {
    margin-left: 10px;
    float: left;
    position: inherit;
  }
  .navbar .brand{
    text-align:center;
  }
  .navbar-inner {
    padding-right: 10px;
  }

  .breadcrumb {
    margin: 0 0 10px;
    padding: 7px 10px;
  }
  article {
    padding: 6px 16px;
  }
