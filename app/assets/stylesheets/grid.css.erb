  }
  .input-xlarge, input.xlarge, textarea.xlarge, select.xlarge {
      width: 200px;
  }
  .categories .box {
    padding: 3px 5px 5px 1px;
    height: auto;
    min-height: 200px;
  }
  ul.nav li#home_link {
    display:none;
  }
  span.noroll {
    text-overflow: ellipsis;
    display: block;
    white-space: normal;
    overflow: visible;
  }
  article section {
    font-size: 13px;
  }
  .content article img.show_item_image,
  .content article img.youtube {
    display: block;
  }
  article section span#item_body img {
    max-width: 320px;
    max-height: 280px;
  }
  #item_body {
    display: block;
    clear: both;
  }
  article footer {
    padding: 20px 0 40px 0;
  }
