.clearfix {
  *zoom: 1;
}

.clearfix:before,
.clearfix:after {
  display: table;
  content: "";
}

.clearfix:after {
  clear: both;
}

.hide-text {
  font: 0/0 a;
  color: transparent;
  text-shadow: none;
  background-color: transparent;
  border: 0;
}

.input-block-level {
  display: block;
  width: 100%;
  min-height: 28px;
  -webkit-box-sizing: border-box;
     -moz-box-sizing: border-box;
      -ms-box-sizing: border-box;
          box-sizing: border-box;
}

.hidden {
  display: none;
  visibility: hidden;
}

.visible-phone {
  display: none !important;
}

.visible-old-phone {
  display: none !important;
}

.visible-tablet {
  display: none !important;
}

.hidden-desktop {
  display: none !important;
}

@media (max-width: 320px) {
  .hidden-old-phone {
    display: none !important;
  }
  .visible-old-phone {
    display: inherit !important;
  }
}

@media (max-width: 767px) {
  .visible-phone {
    display: inherit !important;
  }
  .hidden-phone {
    display: none !important;
  }
  .hidden-desktop {
    display: inherit !important;
  }
  .visible-desktop {
    display: none !important;
  }
}

@media (min-width: 768px) and (max-width: 979px) {
  .visible-tablet {
    display: inherit !important;
  }
  .hidden-tablet {
    display: none !important;
  }
  .hidden-desktop {
    display: inherit !important;
  }
  .visible-desktop {
    display: none !important ;
  }
}












@media only screen and (min-width: 640px) {

  body {
    /*padding-top: 60px;*/
    font-size: 13px;
  }
  .container-fluid {
     min-width: 640px;
  }
  .container-fluid > .sidebar {
    float: left;
    width: 208px;
    left: 25px;
    position: absolute;
    display: block;
  }
  .container-fluid > .content {
    margin-left: 230px;
  }
  .navbar div > ul, .nav {
    font-size: 14px;
  }
  div.well form {
    padding: 5px 5px 5px 10px;
    margin-bottom: 2px;
  }
  div.well form input#q {
    /*width: 130px;*/
    margin-bottom: 10px;
  }
  span.noroll {
    text-overflow: ellipsis;
    display: block;
    white-space: nowrap;
    overflow: hidden; 
  }
  article section {
    font-size: 13px;
  }
  article section span#item_body img {
    max-width: 460px;
    max-height: 400px;
  }
  article footer {
    padding: 20px 0 40px 0;
  }
  footer ul.footer_menu li a {
    display: inline-block;
  }
  .footer_categories {
    display: none;
    margin-bottom: 0px;
  }
}

