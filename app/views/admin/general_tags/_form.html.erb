<%= semantic_form_for [:admin, @general_tag]  do |f| %>
  <%= f.inputs title: "Required Info" do %>
    <%= f.input :title, label: "Tag Name", required: true %>
  <% end %>
  <%= f.actions do %>
    <%= f.action :submit, button_html: {class: "primary", data: {disable_with: 'Sending...'}} %>
  <% end %>
<% end %>